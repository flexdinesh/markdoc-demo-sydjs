---
type Props = {
  url: string;
  label: string;
};
const { url, label } = Astro.props;
---

<div>
  <iframe
    style="width:60%; aspect-ratio: 16 / 9;"
    src={url}
    title={label}
    frame-border="0"
    allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
    allowfullscreen
  >
  </iframe>
</div>